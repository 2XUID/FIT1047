                Add SixTeen
                Store originaddr
                Jns PrintString
                
prtifis310,		HEX 0
				Load originaddr
                Subt ThirdTwoZero
				Skipcond 000
                Jns prtifis320
                Jns prtyesitis310

prtyesitis310,		HEX 0
