

loop,		JnS subInputString
   			LoadI addr
            STORE Element
            /300 to 301
            Load addr
            Add ONE
            Store addr
            /printout
            
            /zero detection
            Load Element
            Subt Zero
			Skipcond 400
            Jns dollardetect
            Jns ZeroD
            
            /$ detection
            


/Input Function
subInputString, 	HEX 0
    			Input
                Store Element
    			StoreI addr
           		JumpI subInputString



			
ZeroD, 			HEX 0
				Load addr
				Subt ONE
                Subt ONE
                Store addr
                Loadi addr
                Subt Zero
                Skipcond 400
                Jns addradder
                Jns addline     
                
addradder, 		HEX 0
				Load addr
 				Add ONE
                Add ONE
                Store addr
                Jump loop

addline, 		HEX 0
				Jns ifis300
 				
                
ifis300, 		HEX 0
				Load addr
                Subt ThirdOneZero
				Skipcond 000
                Jns ifis310
                Jns yesitis300

yesitis300,		HEX 0
				Load originaddr
                Add SixTeen
                Store addr
                Jump loop
                
ifis310,		HEX 0
				Load addr
                Subt ThirdTwoZero
				Skipcond 000
                Jns ifis320
                Jns yesitis310

yesitis310,		HEX 0
				Load ThirdOneZero
                Add SixTeen
                Store addr
                Jump loop
                
ifis320,		HEX 0
				Load addr
                Subt ThirdThirdZero
				Skipcond 000
                Jump loop
                Jns yesitis320
                
yesitis320,		HEX 0
				Load ThirdTwoZero
                Add SixTeen
                Store addr
                Jump loop
                
dollardetect, 	HEX 0 
				Load Element
				Subt  dollar 
            	Skipcond 400
            	Jump loop     
            	JnS Stringuppermain
                
                
/Print Function
PrintString, 	HEX 0
				Load originaddr
   	 			Loadi originaddr
    			Output
                Subt dollar
                Skipcond 400
               	Jns prezeroD
                Halt
                               
prezeroD,       HEX 0
				Load originaddr
   	 			Loadi originaddr
				Subt Zero
                Skipcond 400
                Jns originaddriadder
                Jns PrintZeroD
                        
originaddriadder, HEX 0 
				Load originaddr
                Add ONE
                Store originaddr
                Jns	PrintString
                
PrintZeroD, 	HEX 0
				Load originaddr
				Subt ONE
                Store originaddr
                Loadi originaddr
                Subt Zero
                Skipcond 400
                Jns originaddriadd2
                Jns Printaddline
                                
originaddriadd2, HEX 0 
				Load originaddr
                Add ONE
                Add ONE
                Store originaddr
                Jns	PrintString

Printaddline, 	HEX 0
				Load newline
				output
                Load originaddr
                Jns printaddline
				
           
printaddline, 	HEX 0
				Jns prtifis300
           
           
           
prtifis300, 		HEX 0
				Load originaddr
                Subt ThirdOneZero
				Skipcond 000
                Jns prtifis310
                Jns prtyesitis300

prtyesitis300,		HEX 0
