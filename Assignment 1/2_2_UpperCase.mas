/ upper turning part
Stringuppermain, 	HEX 0
				
				Load addrofupper
                Loadi addrofupper
                Subt dollar
                Skipcond 400
               	Jns preZerocheck
                Jns PrintString
                
preZerocheck, 		HEX 0
				Load addrofupper
   	 			Loadi addrofupper
				Subt Zero
                Skipcond 400
                Jns Stringupper
                Jns Zerocheck

Stringupper, 	HEX 0 
				Load addrofupper
                Loadi addrofupper
                Subt turnupper
                Storei addrofupper
                Jns addrofupperadder
                
addrofupperadder, HEX 0 
				Load addrofupper
                Add ONE
                Store addrofupper
                Jns	Stringuppermain
                
Zerocheck, 		Hex 0
				Load addrofupper
				Subt ONE
                Store addrofupper
                Loadi addrofupper
                Subt Zero
                Skipcond 400
                Jns addrofupperadder2
                Jns Skipupper

Skipupper, 		HEX 0
				Jns upperaddline

                
addrofupperadder2, 	HEX 0
					Load addrofupper
               	 	Add ONE
                    Add ONE
                	Store addrofupper
                	Jns	Stringuppermain


upperaddline, 	HEX 0
				Jns upperifis300
           
           
           
upperifis300, 		HEX 0
				Load addrofupper
